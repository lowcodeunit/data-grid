{
  "name": "@lowcodeunit/data-grid",
  "version": "0.0.3",
  "scripts": {
    "build:common": "ng build common",
    "build:demo": "ng build demo --prod",
    "build:lcu": "ng build lcu --prod --single-bundle && npm run pack",
    "demo": "npm run build:common && ng serve demo",
    "demo:wc": "npm run build:common && npm run build:lcu && ng serve demo",
    "deploy": "npm version patch && npm run deploy:all",
    "deploy:all": "npm run deploy:lcu && npm run deploy:common && npm run deploy:demo",
    "deploy:common": "npm version patch --prefix projects/common && ng build common && npm publish ./dist/common --access public",
    "deploy:demo": "npm version patch --prefix projects/demo/ && npm run build:demo && npm publish ./dist/demo --access public",
    "deploy:lcu": "npm version patch --prefix projects/lcu/ && npm run build:lcu && npm publish ./dist/lcu --access public",
    "ng": "ng",
    "pack": "npm run pack:lcu",
    "pack:join": "concat-glob-cli -f \"dist/wc/lcu/lcu.*.js\" -o dist/wc/data-grid.lcu.js",
    "pack:lcu": "mkdirp dist/wc/lcu && npm run pack:main && npm run pack:pollyfills && npm run pack:join",
    "pack:main": "concat-glob-cli -f \"dist/lcu/main.*.js\" -o dist/wc/lcu/lcu.1.js",
    "pack:pollyfills": "concat-glob-cli -f \"dist/lcu/scripts.*.js\" -o dist/wc/lcu/lcu.0.js",
    "start": "ng serve demo --port=4210",
    "build": "npm run build:common && npm run build:lcu",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "npx-build-plus:copy-assets": "node projects/lcu/copy-wc-polyfill.js",
    "docs": "./node_modules/.bin/compodoc -p projects/common/tsconfig.lib.json -s -w -o -r 4202 --theme postmark"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^10.1.0",
    "@angular/cdk": "^10.2.0",
    "@angular/common": "^10.1.0",
    "@angular/compiler": "^10.1.0",
    "@angular/core": "^10.1.0",
    "@angular/elements": "^10.1.0",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^10.1.0",
    "@angular/material": "^10.2.0",
    "@angular/platform-browser": "^10.1.0",
    "@angular/platform-browser-dynamic": "^10.1.0",
    "@angular/router": "^10.1.0",
    "@aspnet/signalr": "^1.1.4",
    "@lcu/common": "^1.65.606",
    "@lowcodeunit-devkit/ng-lcu": "^1.24.148",
    "@lowcodeunit/lazy-element": "^1.3.4",
    "@lowcodeunit/lcu-guided-tour-common": "^1.22.204",
    "@webcomponents/custom-elements": "^1.4.3",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "copy": "^0.3.2",
    "core-js": "^3.8.1",
    "ngx-build-plus": "^10.1.1",
    "rxjs": "~6.6.3",
    "terser": "^5.5.1",
    "tslib": "^2.0.3",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.4",
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular/cli": "^10.1.0",
    "@angular/compiler-cli": "^10.1.0",
    "@angular/language-service": "^10.1.0",
    "@lcu/cli": "^1.7.97",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.12",
    "codelyzer": "^6.0.1",
    "concat-glob-cli": "^0.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "mkdirp": "^1.0.4",
    "ng-packagr": "^11.0.3",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tsickle": "^0.39.1",
    "tslib": "^2.0.3",
    "tslint": "~6.1.3",
    "typescript": "^4.0.5"
  }
}
